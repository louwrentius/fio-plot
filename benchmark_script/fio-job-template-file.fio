[iotest]
rw=${RW}
blocksize=${BLOCKSIZE}
size=${SIZE}
directory=${DIRECTORY}
filename=${FILE}
ioengine=libaio
iodepth=${IODEPTH}
direct=1
invalidate=1
refill_buffers=1
randrepeat=0
time_based=1
runtime=${RUNTIME}
numjobs=${NUMJOBS}
group_reporting=1
write_bw_log=${OUTPUT}/${RW}-iodepth-${IODEPTH}-numjobs-${NUMJOBS}
write_lat_log=${OUTPUT}/${RW}-iodepth-${IODEPTH}-numjobs-${NUMJOBS}
write_iops_log=${OUTPUT}/${RW}-iodepth-${IODEPTH}-numjobs-${NUMJOBS}
log_avg_msec=250

